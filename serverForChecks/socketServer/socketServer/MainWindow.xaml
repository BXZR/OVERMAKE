<Window x:Class="socketServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="555" Width="1027" Closed="Window_Closed">
    <Grid Margin="0,0,-8,0">
        <Button Content="save" Height="22" HorizontalAlignment="Right" Margin="0,0,25,344" Name="button1" VerticalAlignment="Bottom" Width="75" Click="button1_Click" />
        <Button Content="start" Height="23" HorizontalAlignment="Right" Margin="0,66,25,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        <Button Content="stop" Height="23" HorizontalAlignment="Right" Margin="0,112,25,0" Name="button3" VerticalAlignment="Top" Width="75" Click="button3_Click_1" />
        <Button Content="step" Height="23" HorizontalAlignment="Right" Margin="0,0,25,128" Name="button4" VerticalAlignment="Bottom" Width="75" Click="button4_Click" />
        <Grid Margin="10,10,166,76" Name="Chart">
            <Label Content="counts" Height="75" Margin="10,10,425,0" Name="theStepLabel" VerticalAlignment="Top" />
            <TextBox x:Name="POSITION" HorizontalAlignment="Left" Height="339" Margin="10,90,0,0" TextWrapping="Wrap" Text="POSITION  [start at (0,0)]:" VerticalAlignment="Top" Width="398" IsReadOnlyCaretVisible="True"/>
            <Canvas x:Name="theCanvas" Margin="431,10,-20,-49" Background="#FFF0FFFE" HorizontalAlignment="Center" VerticalAlignment="Center" Height="478" Width="440" RenderTransformOrigin="0.504,0.503">
                <Canvas.LayoutTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Canvas.LayoutTransform>
            </Canvas>
        </Grid>
        <Button Content="angel" Height="23" HorizontalAlignment="Right" Margin="0,0,25,177" Name="button6" VerticalAlignment="Bottom" Width="75" Click="button6_Click" />
        <Button x:Name="button" Content="tensorFlowDEMO" HorizontalAlignment="Left" Margin="21,470,0,0" VerticalAlignment="Top" Width="373" Click="button_Click"/>
        <Button x:Name="button7" Content="createPic" HorizontalAlignment="Left" Margin="927,456,0,0" VerticalAlignment="Top" Width="75" Click="button7_Click"/>
        <Label x:Name="Charts" Content="Charts" Margin="0,0,87,205" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="47"/>
        <Label x:Name="Charts_Copy" Content="Tools" Margin="0,0,87,473" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="47"/>
        <Label x:Name="Charts_Copy1" Content="Demos" Margin="0,0,87,76" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="57"/>
        <Button x:Name="button5" Content="color" Margin="0,0,25,298" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="button5_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <GradientStop Color="#FFF3F3F3" Offset="0"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <CheckBox x:Name="drawWithBufferCheck" Content="drawWithBuffer" HorizontalAlignment="Right" Margin="0,0,24,259" Height="16" VerticalAlignment="Bottom" IsChecked="True"/>
    </Grid>
</Window>
