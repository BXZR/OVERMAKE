        <Label x:Name="theShowLabel" Content="information" Margin="20,0,19,59" Background="{x:Null}" Foreground="#FF1207FF" HorizontalContentAlignment="Center" Height="178" VerticalAlignment="Bottom"/>
        <Label x:Name="theShowLabel_Copy" Content="PDR With Smartphone" Margin="0,0,30,236" Background="{x:Null}" Foreground="#FF1207FF" Height="25" VerticalAlignment="Bottom" HorizontalContentAlignment="Center" HorizontalAlignment="Right" Width="347" FontWeight="Bold"/>

    </Grid>
